{
    "name": "angular2-boiler-plate",
    "version": "1.0.0",
    "description": "Angular 2 Boiler plate that integrated with WebPack, TypeScript, DatePicker, RxJs for Reactive Extensions in Java Script  , Karma , Jasmine for unit testing ,  WebKit ,protractor e2e testing",
    "scripts": {
        "start": "webpack-dev-server --inline --progress --port 4343",
        "test": "karma start",
        "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
        "tslint": "tslint",
        "typedoc": "typedoc",
        "lint": "npm run tslint \"src/**/*.ts\"",
        "ci": "npm run lint && npm test && npm run e2e",
        "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
        "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ineffablep/angular2-boiler-plate.git"
    },
    "keywords": [
        "Angular2",
        "Biler",
        "Plate",
        "boiler-plate",
        "TypeScript",
        "WebPack",
        "Web",
        "Pack",
        "Karma",
        "Jasmine",
        "Protractor",
        "Material",
        "Design",
        "Unit",
        "Testing",
        "Angular",
        "Reactive",
        "Rx"
    ],
    "author": "Sireesh Pangaluri",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ineffablep/angular2-boiler-plate/issues"
    },
    "homepage": "https://github.com/ineffablep/angular2-boiler-plate#readme",
